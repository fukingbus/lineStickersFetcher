{
  "name": "kneden",
  "version": "1.0.2",
  "description": " Transpile ES7 async/await to vanilla ES6 Promise chains",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marten-de-vries/babel-plugin-kneden.git"
